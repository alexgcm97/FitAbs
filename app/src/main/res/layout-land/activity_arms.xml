<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="4dp"
        android:text="@string/armsEx"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgArms1"
        android:layout_width="124dp"
        android:layout_height="68dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgArms2"
        android:layout_width="124dp"
        android:layout_height="68dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgArms1" />

    <ImageView
        android:id="@+id/imgArms3"
        android:layout_width="124dp"
        android:layout_height="68dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgArms2"/>

    <TextView
        android:id="@+id/textView60"
        android:layout_width="395dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:text="@string/armsEx1"
        app:layout_constraintStart_toEndOf="@+id/imgArms1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="56dp"
        android:text="@string/armsName1"
        app:layout_constraintStart_toEndOf="@+id/imgArms1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView62"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="@string/armsName2"
        app:layout_constraintStart_toEndOf="@+id/imgArms2"
        app:layout_constraintTop_toBottomOf="@+id/textView60" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="395dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:text="@string/armsEx2"
        app:layout_constraintStart_toEndOf="@+id/imgArms2"
        app:layout_constraintTop_toBottomOf="@+id/textView62" />

    <TextView
        android:id="@+id/textView64"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/armsName3"
        app:layout_constraintStart_toEndOf="@+id/imgArms3"
        app:layout_constraintTop_toBottomOf="@+id/textView63" />

    <TextView
        android:id="@+id/textView65"
        android:layout_width="395dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:text="@string/armsEx3"
        app:layout_constraintStart_toEndOf="@+id/imgArms3"
        app:layout_constraintTop_toBottomOf="@+id/textView64" />
</android.support.constraint.ConstraintLayout>